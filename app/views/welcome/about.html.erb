  <div class="container" id="formOverlay">
    <%= render partial: 'pulsechannel_form', locals: {pulsechannel: @pulsechannel, admright: @admright} %>
    <%= render partial: 'filter_form', locals: {pulsechannel: @pulsechannel, admright: @admright} %>
  </div>

  <div id="map" class="mainMap"></div>

  <div id="data_channels" data-channel-data="<%= @channels.to_json %>"></div>

<script>
  gon.inChannel = undefined
  var channelsAll = $('#data_channels').data('channel-data')
  $('#create-channel-form').click(function(){
    $('#create-channel-dropdown').slideDown( "slow");
  })

  $('#channelFilter').click(function(){
    $('#filter-dropdown').slideDown( "slow");
  })

  $('#eventTimePicker').datetimepicker()

  $('.new_pulsechannel').submit( function(){
    var eventStartTime = $('#eventTimePicker').val()
  })

  $('select').material_select()

  $(document).ready(function(){
    $(".dropdown-button").dropdown();
  });
</script>
